ch\">\n        {hits.map((i) => (\n          <div className=\"search-result\">\n            <Link to={\"/\" + category + \"/\" + i.cpvsearch}>\n              <h3>{i.cpvmainsearch}</h3>\n            </Link>\n            <Link to={\"/\" + category + \"/\" + i.cpvnumber + \"/\" + i.objectID}>\n              {/* <p>\n                {i.tittel.replace(query, () => {\n                  return query.bold();\n                })}\n              </p> */}\n              <h4>{i.cpvnumber}</h4>\n              <p>{i.tittel}</p>\n            </Link>\n          </div>\n        ))}\n      </div>\n    </div>\n  );\n}\n\nexport default AlgoliaSearch;\n","/Users/evy/Documents/GitHub/doffindata/src/components/Search.jsx",[],["158","159"],"/Users/evy/Documents/GitHub/doffindata/src/components/SearchData.jsx",[],"/Users/evy/Documents/GitHub/doffindata/src/components/homeIcons.jsx",["160","161"],"import React, { useEffect, useState } from 'react'\nimport { homeIcons } from '../helpers/functions'\nimport { Link } from 'react-router-dom'\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\nimport { faArrowRight } from '@fortawesome/free-solid-svg-icons'\n\nconst HomeIcons = ({ i }) => {\n  return (\n    <Link className='main-boxes' to={i.main}>\n      <div style={homeIcons(i.main).style} className='homeicon-container'>\n        <FontAwesomeIcon id='homeicon-icon' icon={homeIcons(i.main).icon} />\n      </div>\n      <div className='main-box'>\n        <div className='main-box-text'>\n          <p className='main-boxes-title'>{i.main}</p>\n          {/* <p className='main-boxes-cpv'>CPV {i.children[0].code}</p> */}\n        </div>\n        <div className='main-boxes-arrow'>\n          <FontAwesomeIcon icon={faArrowRight} color='#4DBFAC' />\n        </div>\n      </div>\n    </Link>\n  )\n}\nexport default HomeIcons\n","/Users/evy/Documents/GitHub/doffindata/src/components/SavedSearch.jsx",["162"],"import React, { useContext, useEffect, useState } from 'react'\nimport { faChevron